$env:MACHINE = "{{ .machinePath }}"
$env:MACHINE_ID = "{{ .machine }}"
$env:MACHINE_PRIVATE = "{{ .privatePath }}"

if (Test-Path -Path "$env:MACHINE/machines/$env:MACHINE_ID/profile.ps1") {
    . "$env:MACHINE_PRIVATE/profile.ps1"
}
. "$env:MACHINE/config/shell/profile.ps1"
if (Test-Path -Path "$env:MACHINE/machines/$env:MACHINE_ID/profile.ps1") {
    . "$env:MACHINE/machines/$env:MACHINE_ID/profile.ps1"
}
